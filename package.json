{
  "name": "moment-opening-times",
  "version": "2.0.0",
  "description": "A small class to determine the status of a given moment in relation to a set of opening times",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhsuk/moment-opening-times"
  },
  "main": "OpeningTimes.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-moment": "^0.1.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.3.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-watch": "^2.1.11",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "scripts": {
    "test": "TZ=GMT mocha",
    "watch-test": "npm run test -- --watch --reporter min",
    "lint": "eslint --ext .js,.json --max-warnings 0 .",
    "generate-coverage": "istanbul cover _mocha -- --recursive test",
    "check-coverage": "istanbul check-coverage --config .istanbul.yml",
    "upload-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "moment",
    "opening",
    "times"
  ],
  "author": "Neil McLaughlin & Steven Hunt",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "git stash --keep-index && npm run lint && npm run test && git stash pop",
      "post-checkout": "npm install"
    }
  }
}
